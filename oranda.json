{
  "project": {
    "name": "rsql"
  },
  "build": {
    "path_prefix": "rsql"
  },
  "components": {
    "changelog": {
      "read_changelog_file": false
    },
    "artifacts": {
      "package_managers": {
        "preferred": {
          "linux/mac shell": "curl --proto '=https' --tlsv1.2 -LsSf https://github.com/theseus-rs/rsql/releases/latest/download/rsql_cli-installer.sh | sh",
          "windows powershell": "powershell -c \"irm https://github.com/theseus-rs/rsql/releases/latest/download/rsql_cli-installer.ps1 | iex\"",
          "homebrew": "brew install theseus-rs/homebrew-tap/rsql_cli",
          "cargo": "cargo install rsql_cli",
          "binstall": "cargo binstall rsql_cli"
        }
      }
    }
  },
  "styles": {
    "theme": "dark",
    "logo": "rsql_cli/resources/rsql.jpg"
  }
}
